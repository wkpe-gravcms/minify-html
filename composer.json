{
  "name": "wkpe-gravcms/minify-html",
  "description": "a gravcms plugin to minify HTML output",
  "license": "MIT",
  "type": "grav-plugin",
  "authors": [
    {
      "name": "Wasseem Khayrattee",
      "email": "hey@wk.contact"
    },
    {
      "name": "Jim Blue"
    }
  ],
  "keywords": ["gravcms", "plugin", "html compress"],
  "require": {
    "wyrihaximus/html-compress": "4.2.1"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.4"
    }
  },
  "autoload": {
    "psr-4": {
      "Grav\\Plugin\\MinifyHtml\\": "classes/plugin"
    },
    "classmap":  ["minify-html.php"]
  }
}
